<h3>Student Form</h3>
<%= form_tag students_path do %>
  <label>Student Name</label>
  <label>First: </label><input type="text" id="first_name" name="student[f_name]">
  <label>Last: </label><input type="text" id="last_name" name="student[l_name]">

  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <%= submit_tag "Submit Student" %>
<% end %>
